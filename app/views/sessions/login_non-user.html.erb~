<head>
<h1>Sign In</h1>

<%= form_for(:session, :url => sessions_path) do |f| %>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <div class="field">
    <%= f.label :password %><br />
    <%= f.password_field :password %>
  </div><br>
  <div class="actions">
    <%= f.submit "Sign In" %>
  </div>
<% end %>

